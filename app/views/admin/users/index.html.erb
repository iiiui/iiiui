<div class="row-fluid">
  <div class="widget_container">
    <div class="well nomargin">
      <div class="navbar navbar-static navbar_as_heading">
        <div class="navbar-inner">
          <div class="container" style="width: auto;">
            <a class="brand">Pages</a>
            <a class="btn pull-right" href="add-new-article.html">Add New</a>
          </div>
        </div>
      </div>
      <div class="subnav">
        <form class="navbar-search pull-right" action="">
          <input type="text" class="search-query" placeholder="Search">
        </form>
      </div>

      <table class="table smallfont">
        <thead>
          <tr>
            <td style="width:20px;"><input type="checkbox" id="chkbck" /></td>
            <td>Name</td>
            <td>Email</td>
            <td>Signed up Date</td>
            <td>LastSignin Date</td>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><input type="checkbox" class="chkbck" /></td>
              <td><%= link_to user.name, user %></td>
              <td><%= link_to user.email, user %></td>
              <td><%= user.created_at.to_date %> </td>
              <td><%= user.last_sign_in_at.to_date %> </td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <div class="subnav nobg">
        <div class="span4">
          <%= will_paginate %>
        </div>
      </div>
    </div>
  </div>
</div>

