<%= simple_form_for(@order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :item_id %>
    <%= f.input :count %>
    <%= f.input :order_from %>
    <%= f.input :total_fee %>
    <%= f.input :price %>
    <%= f.input :trade_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
